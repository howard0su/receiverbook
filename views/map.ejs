<%- contentFor('head') %>
<script src="https://maps.googleapis.com/maps/api/js?key=<%= google_maps_api_key %>"></script>
<script src="/static/js/map.js"></script>
<script>
    $(function(){
        var receivers = <%- JSON.stringify(receivers.map(inp => {
            var out = {};
            ['label', 'location', 'version', 'url'].forEach(function(key){
                out[key] = inp[key];
            });
            out['type'] = inp.getPresentationType();
            return out;
        })) %>;
        $('.content-container').addReceivers(receivers);
    });
</script>
<%- contentFor('title') %>
Map
<%- contentFor('html-class') %>
map