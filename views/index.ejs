<%
function getReceiverType(receiver) {
    switch (receiver.type) {
        case 'openwebrx':
            return 'OpenWebRX';
        case 'websdr':
            return 'WebSDR';
        case 'kiwisdr':
            return 'KiwiSDR';
    }
    return 'Other';
}
%>
<h1>Receiver list</h1>
<table class="table mt-5">
    <% receivers.forEach((receiver) => { %>
        <tr>
            <td>
                <h5><%= receiver.label %></h5>
                <div><a href="<%= receiver.url %>" target="_blank"><%= receiver.url %></a></div>
                <div><%= getReceiverType(receiver) %> <%= receiver.version %></div>
            </td>
        </tr>
    <% }); %>
</table>